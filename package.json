{
  "name": "ide-java",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Java language support for Atom-IDE.",
  "repository": "https://github.com/atom/ide-java",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0"
  },
  "configSchema": {
    "java.home": {
      "type": "string",
      "description": "Absolute path to JDK 8 home folder used to launch the Java Language Server."
    }
  },
  "dependencies": {
    "atom-languageclient": "^0.1.0",
    "decompress": "^4.0.0"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-code-format.provider": {
      "versions": {
        "0.0.0": "provideCodeFormat"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    }
  }
}
